{
  "name": "learning-git-2",
  "version": "1.0.0",
  "description": "https://endriu17.github.io/learning-git-2/",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "globstar node_modules/nu-html-checker/bin/cli *.html",
    "init-project": "npm install && npm run init:dirs && npm run init:files && npm run init:gitignore",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:sass": "node_modules/node-sass/bin/node-sass --output-style compact -o css sass",
    "build:autoprefixer": "node_modules/autoprefixer-cli/autoprefixer-cli -o css/style.css",
    "build-dev:sass": "node_modules/node-sass/bin/node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:browsersync": "node_modules/.bin/browser-sync start --server --files css/*.css *.html --watch onchange reload",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass --watch",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endriu17/learning-git-2.git"
  },
  "keywords": [],
  "author": "Andrzej Olaszewski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/endriu17/learning-git-2/issues"
  },
  "homepage": "https://github.com/endriu17/learning-git-2#readme",
  "dependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.24.5",
    "globstar": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nu-html-checker": "^0.1.0"
  },
  "devDependencies": {
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3"
  }
}
